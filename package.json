{
  "name": "wysiwyg-editor-toolkit",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "elm make --docs=documentation.json",
    "test": "elm-test && npm run-script build && npm run-script build-example && elm-format --validate .",
    "build-example": "(cd example && elm make src/Main.elm)",
    "watch": "chokidar --initial elm.json 'src/**/*.elm' 'tests/**/*.elm' 'example/**/*.elm' -c 'npm test'",
    "watch-example": "(cd example && elm reactor --port=8081)"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "chokidar-cli": "^1.2.1",
    "elm": "^0.19.0-bugfix6",
    "elm-format": "^0.8.1",
    "elm-test": "^0.19.0-rev4"
  }
}
